{
  "name": "@tippingchain/sdk",
  "version": "2.0.1",
  "type": "commonjs",
  "description": "TippingChain SDK v2.0 - Multi-chain tipping with USDC payouts on ApeChain",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "author": "TippingChain Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tippingchain/tippingchain.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/tippingchain/tippingchain#readme",
  "bugs": {
    "url": "https://github.com/tippingchain/tippingchain/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "./node_modules/.bin/tsc --noEmit",
    "prepublishOnly": "rm -rf dist && npm run typecheck && npm run build",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "apechain",
    "tipping",
    "crypto",
    "multi-chain",
    "web3",
    "ethereum",
    "polygon",
    "optimism",
    "bsc",
    "cross-chain",
    "defi"
  ],
  "peerDependencies": {
    "thirdweb": "^5.0.0",
    "react": "^18.0.0"
  },
  "dependencies": {
    "@tippingchain/contracts-interface": "^1.2.0",
    "ethers": "^6.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}